{
  "name": "reenv",
  "version": "0.1.0",
  "description": "dotenv-cli written in reason",
  "esy": {
    "build": "pesy",
    "release": {
      "releasedBinaries": [
        "reenv.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "reenv.lib"
      ],
      "main": "TestReenv",
      "name": "TestReenv.exe"
    },
    "library": {
      "name": "reenv.lib",
      "namespace": "Reenv",
      "require": [
        "unix",
        "str"
      ]
    },
    "executable": {
      "require": [
        "reenv.lib"
      ],
      "main": "ReenvApp",
      "name": "reenv.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x TestReenv.exe"
  },
  "dependencies": {
    "@opam/dune": ">=1.6.0",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "^4.4.0",
    "pesy": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
