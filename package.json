{
  "name": "reenv",
  "version": "0.3.0",
  "description": "dotenv-cli written in reason",
  "esy": {
    "build": "dune build --profile=release",
    "install": [
      "esy-installer reenv.install",
      "esy-installer reenv-tests.install"
    ],
    "release": {
      "bin": [
        "reenv"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "rely.lib",
        "reenv.lib"
      ],
      "main": "TestReenv",
      "name": "TestReenv.exe"
    },
    "library": {
      "name": "reenv.lib",
      "namespace": "Reenv",
      "require": [
        "unix",
        "str"
      ]
    },
    "executable": {
      "require": [
        "reenv.lib"
      ],
      "main": "ReenvApp",
      "name": "reenv.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x RunTests.exe",
    "coverage": "esy bisect-ppx-report -html _coverage -I '#{self.target_dir}' bisect*.out -verbose",
    "doc": "esy dune build @doc",
    "format": "esy dune build @fmt --auto-promote"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@esy-ocaml/reason": ">= 3.4.0 < 3.5.0",
    "refmterr": "*",
    "ocaml": "^4.4.0",
    "pesy": "*",
    "@reason-native/pastel": "*",
    "@reason-native/rely": "*",
    "@opam/cmdliner": "*",
    "@opam/ppxlib": "*",
    "@opam/re": "*"
  },
  "resolutions": {
    "@opam/ppxlib": "ocaml-ppx/ppxlib:ppxlib.opam#3bdccbb09b94acdf1f29e3cf57a263aea82e72fa",
    "@opam/ocaml-migrate-parsetree": "ocaml-ppx/ocaml-migrate-parsetree:ocaml-migrate-parsetree.opam#a5be5e1b94f02c667ab354c2e6b55baedeb30187",
    "@opam/base": "v0.11.1"
  },
  "devDependencies": {
    "@opam/bisect_ppx": "*",
    "@opam/merlin": "*",
    "ocaml": "~4.6.0",
    "@opam/odoc": "1.4.0"
  }
}
